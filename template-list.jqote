<div class="template-container template-list">
	<div class="header"> <div class="title">Template list</div><div class="minibutton close" data-tooltip="Close" aria-label="Close" data-tooltip-delay="800"></div></div>
	<ol>
		<% for(var i = 0; i<this.length; i++) { var template = this.at(i); %>
		<li template-id="<%=template.id %>"><div class="template-item" data-tooltip="Insert template" aria-label="Insert template" data-tooltip-delay="800"><%=template.get("name") %></div><div class="minibutton delete" data-tooltip="Delete template" aria-label="Delete template" data-tooltip-delay="800"></div><div class="minibutton update" data-tooltip="Edit template" aria-label="Edit template" data-tooltip-delay="800"></div></li>
		<% } %>
	</ol>
	<div class="bottom"><div class="button new" data-tooltip="Create new template" aria-label="Create new template">New</div></div>
</div>
